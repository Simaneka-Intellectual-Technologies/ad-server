var headers = {
  'authorisation': 'XUwCeuxr6ZjELa4cA9dhYSDV7fpsRMGz',
  'Cookie': 'ci_session=5f355c0042c75425ca6d8dce3cf0560109092bb2'
};
var request = http.Request('POST', Uri.parse('https://ad.simaneka.com/api/get?type=Vertical Strip'));

request.headers.addAll(headers);

http.StreamedResponse response = await request.send();

if (response.statusCode == 200) {
  print(await response.stream.bytesToString());
}
else {
  print(response.reasonPhrase);
}